{
    "headers": [
        {
            "source": "/static/css/(.*).css",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/css"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)/static/css/(.*).css",
            "destination": "/static/css/$2.css"
        }
    ]
}